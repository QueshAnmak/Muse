{
    "name": "Muse",
    "description": "The Chromium extension to start Muse.",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "nativeMessaging",
        "tabs",
        "scripting"
    ],
    "background": {
        "service_worker": "./core/background.js"
    },
    "action": {
        "default_title": "Activate Muse"
    }
}