{
    "name": "Meet Bot",
    "description": "The Chromium extension to start Meet Bot.",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "nativeMessaging",
        "tabs",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    }
}